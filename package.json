    "build",
    "properties"
  ],
  "scripts": {
    "build": "node ./split-files.js && node ./build.js",
    "clean": "rm -rf build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-typography-test": "node index.js design/tokens.json temp/typography.json base/options,base/sizing,core/typography base/options,base/sizing",
    "build-light-test": "node index.js design/tokens.json temp/dark.json base/options,base/sizing,themes/dark,core/shadows base/options,base/sizing",
    "build-dark-test": "node index.js design/tokens.json temp/light.json base/options,base/sizing,themes/light,core/shadows base/options,base/sizing",
    "build-folder-test": "node index.js tokens temp/folder-light.json base/options,base/sizing,themes/light,core/shadows base/options,base/sizing",
    "build-resolve-false-test": "node index.js design/tokens.json temp/resolve-false.json base/options,base/sizing,themes/dark,core/shadows base/options,base/sizing --resolveReferences=false",
    "ft-test": "npm run build-typography-test && npm run build-light-test && npm run build-dark-test && npm run build-folder-test && npm run build-resolve-false-test && jest"
  },
